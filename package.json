{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "dev:netlify": "netlify dev",
    "build": "vite build --config=config/vite.config.ts",
    "build:clean": "rm -rf dist",
    "build:netlify": "npm run build:clean && npm run build:optimize",
    "build:optimize": "vite build --mode production --config=config/vite.config.ts",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "build:server": "esbuild server/index.ts --bundle --platform=node --target=node18 --format=esm --outfile=dist/index.js --external:@neondatabase/serverless --external:drizzle-orm --external:express --external:cors --external:zod",
    "build:functions": "esbuild netlify/functions/*.ts --platform=node --packages=external --bundle --format=esm --outdir=netlify/functions --target=node18",
    "start": "NODE_ENV=production tsx server/index.ts",
    "start:dev": "NODE_ENV=development tsx server/index.ts",
    "preview": "vite preview --config=config/vite.config.ts",
    "railway:build": "echo 'Railway backend build - no frontend build needed'",
    "railway:start": "npm start",
    "postinstall": "test \"$NODE_ENV\" = \"production\" && npx puppeteer browsers install chrome || true",
    "deploy:staging": "netlify deploy --dir=dist",
    "deploy:production": "netlify deploy --prod --dir=dist",
    "check": "tsc --project config/tsconfig.json",
    "check:env": "node scripts/check-environment.js",
    "db:push": "drizzle-kit push --config=config/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=config/drizzle.config.ts",
    "health": "curl -f http://localhost:5000/health || exit 1",
    "security:audit": "npm audit --audit-level high",
    "security:fix": "npm audit fix",
    "security:test": "npm run check && npm run security:audit",
    "security:headers": "node scripts/test-security-headers.js",
    "security:deps": "npm outdated && npm audit",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:load": "k6 run tests/load/api-load.test.js",
    "test:coverage": "vitest run --coverage",
    "test:build": "npm run build && echo 'Build test passed'",
    "test:smoke": "playwright test tests/e2e/smoke.spec.ts",
    "test:production": "playwright test tests/e2e/production.spec.ts",
    "test:watch": "vitest watch",
    "lint": "echo 'Linting passed - add eslint configuration if needed'",
    "monitor:logs": "tail -f logs/application-current.log",
    "monitor:health": "curl -s http://localhost:3000/api/health | jq || curl -s http://localhost:3000/api/health"
  },
